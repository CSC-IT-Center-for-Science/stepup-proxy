<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>#springMessageText("stepup.title", "Web Stepup Service")</title>
    <link rel="stylesheet" type="text/css" href="$request.getContextPath()/css/main.css">
  </head>
  <body>
    <div class="wrapper">
      <div class="container">
        <header>
          <img src="$request.getContextPath()#springMessage("stepup.logo")" alt="#springMessageText("stepup.logo.alt-text", "logo")">
        </header>
        <div class="content">
        
        #springMessageText("stepup.stepup.failed","You provided wrong response.")
        
       </div>
      </div>
      <footer>
        <div class="container container-footer">
          <p class="footer-text">#springMessageText("stepup.footer", "Insert your footer text here.")</p>
        </div>
      </footer>
    </div>
  </body>
</html>
